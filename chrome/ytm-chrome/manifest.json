{
  "manifest_version": 2,
  "name": "YTMChrome",
  "description": "YTMusd bindings for Chrome.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "YTMChrome"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://www.youtube.com/*"
  ]
}
